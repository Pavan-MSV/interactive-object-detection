      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/detect",
        "run": {
          "serviceId": "object-detection-api",
          "region": "us-central1"
        }
      }
    ]
  }
}
